{
  "expo": {
    "name": "Supervisores",
    "description": "Administrador de planes de trabajos para los supervisores de Unidrogas.",
    "slug": "supervisoresApp",
    "privacy": "public",
    "sdkVersion": "31.0.0",
    "platforms": ["ios", "android"],
    "version": "0.8.7",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "primaryColor": "#00A1E4",
    "splash": {
      "image": "./assets/unidrogas.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.binar10.supervisores",
      "supportsTablet": true
    },
    "android": {
      "package": "com.binar10.supervisores"
    }
  },
  "android": {
    "permissions": [
      "ACCESS_COARSE_LOCATION",
      "ACCESS_FINE_LOCATION"
    ]
  }
}

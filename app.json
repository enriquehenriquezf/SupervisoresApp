{
  "expo": {
    "name": "SuperAT",
    "description": "Administrador de planes de trabajos para los supervisores de Unidrogas.",
    "slug": "SuperAT",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "platforms": ["ios", "android"],
    "version": "1.3.2",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "primaryColor": "#00A1E4",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "enabled": true,
      "checkAutomatically": "ON_ERROR_RECOVERY",
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.binar10.supervisores",
      "supportsTablet": true,
      "buildNumber": "7"
    },
    "android": {
      "package": "com.binar10.supervisores",
      "versionCode": 7
    }
  },
  "android": {
    "permissions": [
      "ACCESS_COARSE_LOCATION",
      "ACCESS_FINE_LOCATION"
    ]
  }
}
